.textTwoLineEllipsis {
    text-overflow: -o-ellipsis-lastline;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical; }
  
  .textOverflowEllipsis {
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    min-width: 0; }
  
  .radientBg {
    background: #01d4c9;
    /* Old browsers */
    background: -moz-linear-gradient(top, #01d4c9 0%, #05b7c5 100%);
    /* FF3.6-15 */
    background: -webkit-linear-gradient(top, #01d4c9 0%, #05b7c5 100%);
    /* Chrome10-25,Safari5.1-6 */
    background: linear-gradient(to bottom, #01d4c9 0%, #05b7c5 100%);
    /* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#01d4c9', endColorstr='#05b7c5',GradientType=0 );
    /* IE6-9 */ }
  
  page {
    height: 100%; }
  
  page .picture-camera {
      height: 100%;
      padding: 30rpx;
      box-sizing: border-box; }
  
  page .picture-camera .picture-camera-warp {
        width: 100%; }
  
  page .picture-camera .picture-camera-warp .camera-warp {
          position: relative;
          width: 100%;
          height: 0;
          padding-bottom: calc( 100% * 1.25); }
  
  page .picture-camera .picture-camera-warp .camera-warp camera, page .picture-camera .picture-camera-warp .camera-warp cover-image {
            position: absolute;
            width: 100%;
            height: 100%;
            left: 0;
            top: 0; }
  
  page .picture-camera .picture-camera-warp .camera-warp cover-image {
            z-index: 2; }
  
  page .picture-camera .picture-camera-warp .tips {
          margin-top: 70rpx;
          font-size: 28rpx;
          color: #aaaaaa;
          text-align: center; }
  
  page .picture-camera .operate {
        margin-top: 86rpx;
        color: #333333;
        font-size: 40rpx; }
  
  page .picture-camera .operate .take .big {
          width: 120rpx;
          height: 120rpx;
          line-height: 120rpx;
          box-sizing: border-box;
          border: 2rpx dashed #f7941f;
          border-radius: 50%;
          vertical-align: middle;
          margin: 0 auto; }
  
  page .picture-camera .operate .take .big .small {
            width: 88rpx;
            height: 88rpx;
            border: 4rpx solid #f7941f;
            border-radius: 50%;
            box-sizing: border-box; }
  
  page .picture-camera .operate image {
          width: 65rpx;
          height: 56rpx; }
  